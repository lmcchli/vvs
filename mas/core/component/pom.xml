                <source>${MAS.directory}/mrdhandler/src</source>
                <source>${MAS.directory}/numberanalyzer/src</source>
                <source>${MAS.directory}/operateandmaintainmanager/src</source>
                <source>${MAS.directory}/profilemanager/src</source>
                <source>${MAS.directory}/provisionmanager/src</source>
                <source>${MAS.directory}/servicerequestmanager/src</source>
                <source>${MAS.directory}/stream/src</source>
                <source>${MAS.directory}/util/src</source>
                <source>${MAS.directory}/migration/src</source>
                <!-- Interfaces -->
                <source>${MAS.directory}/callmanager/interface</source>
                <source>${MAS.directory}/execution_engine/interface</source>
                <source>${MAS.directory}/mailbox/interface</source>
                <source>${MAS.directory}/masagent/interface</source>
