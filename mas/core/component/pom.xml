                <source>${MAS.directory}/mediatranslationmanager/interface</source>
                <source>${MAS.directory}/message_sender/interface</source>
                <source>${MAS.directory}/numberanalyzer/interface</source>
                <source>${MAS.directory}/operateandmaintainmanager/interface</source>
                <source>${MAS.directory}/operateandmaintainmanager/interface</source>
                <source>${MAS.directory}/profilemanager/interface</source>
                <source>${MAS.directory}/stream/interface</source>
                <source>${MAS.directory}/servicerequestmanager/interface</source>
                <source>${MAS.directory}/trafficeventsender/interface</source>
                <source>${MAS.directory}/configurationmanager/interface</source>
                <source>${MAS.directory}/provisionmanager/interface</source>
                <source>${MAS.directory}/externalcomponentregister/interface</source>
                <source>${MAS.directory}/util/interface</source>
              </sources>
            </configuration>
          </execution>
          <execution>
            <id>add-test-source</id>
            <phase>generate-test-sources</phase>
            <goals>
              <goal>add-test-source</goal>
            </goals>
            <configuration>
              <sources>
                <!-- Add other test source folder her for compilation -->
                <source>${MAS.directory}/callmanager/test</source>
                <source>${MAS.directory}/execution_engine/test</source>
                <source>${MAS.directory}/migration/test</source>
              </sources>
            </configuration>
          </execution>
        </executions>
