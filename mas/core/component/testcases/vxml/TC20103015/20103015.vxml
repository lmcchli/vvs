<?xml version="1.0" encoding="UTF-8"?>
<vxml version="2.0" xmlns="http://www.w3.org/2001/vxml">
	<form id="id1">
		<block>
			<throw event="myEvent" messageexpr="'my' + 'Message'" />
		</block>
	</form>
	<catch event="myEvent">
		<if cond="_message == 'myMessage'">
			<log label="error">TC 20103015 Passed</log>				
		<else/>
			<log label="error">TC 20103015 Passed</log>						
		</if>
		<exit/>
	</catch>
</vxml>
